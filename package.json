{
  "name": "prettier-format",
  "version": "3.2.3",
  "description": "auto load config and run prettier on code",
  "homepage": "https://github.com/fisker/prettier-format#readme",
  "bugs": {
    "url": "https://github.com/fisker/prettier-format/issues"
  },
  "repository": "fisker/prettier-format",
  "funding": "https://github.com/fisker/prettier-format?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "build": "echo no need",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean lint build test format dist",
    "test": "ava",
    "test-coverage": "c8 ava",
    "prepare": "husky install"
  },
  "ava": {
    "verbose": true
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "import-meta-resolve": "3.0.0",
    "prettier": "3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@fisker/eslint-config": "11.0.0",
    "@fisker/husky-config": "4.1.3",
    "@fisker/lint-staged-config": "3.3.1",
    "@fisker/prettier-config": "5.1.1",
    "ava": "5.3.1",
    "c8": "8.0.1",
    "del-cli": "5.0.0",
    "eslint": "8.46.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "markdownlint-cli": "0.35.0",
    "npm-run-all": "4.1.5",
    "outdent": "0.8.0",
    "sort-package-json": "2.5.1"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
